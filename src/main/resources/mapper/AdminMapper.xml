<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.com.code.certificateProcessor.mapper.AdminMapper">
    <select id="getAdminAuth" resultType="Admin">
        select username,password,auth from admin where username=#{username}
    </select>

    <insert id="addAdmin" parameterType="Admin">
        insert into admin(username,password,fullName,auth)
        values(#{username},#{password},#{fullName},#{auth})
    </insert>

    <select id="getAdminByUserName" resultType="Admin">
        select id,username,fullName from admin where username=#{username}
    </select>




</mapper>
